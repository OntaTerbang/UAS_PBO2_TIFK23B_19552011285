<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.mycompany.kasirpropertijavafxfadillahfajri.DasborController" spacing="15.0" alignment="CENTER" style="-fx-padding: 20;">
    <children>
        <Label fx:id="welcomeLabel" style="-fx-font-size: 18px;" />
        <HBox spacing="10.0" alignment="CENTER">
            <Button fx:id="btnProperti" text="Kelola Properti" onAction="#goToProperti" />
            <Button fx:id="btnPenyewa" text="Data Diri" onAction="#goToPenyewa" />
            <Button fx:id="btnTransaksi" text="Kelola Transaksi" onAction="#goToTransaksi" />
        </HBox>
        <Button fx:id="btnLogout" text="Logout" onAction="#handleLogout" style="-fx-background-color: #e74c3c; -fx-text-fill: white;" />
    </children>
</VBox>
